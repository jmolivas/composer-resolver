{
    "name": "toflar/composer-resolver",
    "description": "Executing composer update via a Dockerized API.",
    "license": "MIT",
    "authors": [
        {
            "name": "Yanick Witschi",
            "email": "yanick.witschi@terminal42.ch"
        }
    ],
    "config": {
        "platform": {
            "php": "7.1.0"
        }
    },
    "require": {
        "php": "^7.1",
        "silex/silex": "^2.0",
        "composer/composer": "dev-master",
        "predis/predis": "^1.1",
        "predis/service-provider": "^2.0",
        "monolog/monolog": "^1.19",
        "symfony/monolog-bridge": "^3.1",
        "pimple/pimple": "3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.6.*",
        "satooshi/php-coveralls": "2.0.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Toflar\\ComposerResolver\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Toflar\\ComposerResolver\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
