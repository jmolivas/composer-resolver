{
    "name": "toflar/composer-resolver",
    "description": "Executing composer update via a Dockerized API.",
    "license": "MIT",
    "authors": [
        {
            "name": "Yanick Witschi",
            "email": "yanick.witschi@terminal42.ch"
        }
    ],
    "config": {
        "platform": {
            "php": "7.0.7"
        }
    },
    "require": {
        "php": "^7.0",
        "silex/silex": "^2.0",
        "composer/composer": "dev-improve-installer-extensibility",
        "predis/predis": "^1.1",
        "predis/service-provider": "dev-master",
        "monolog/monolog": "^1.19",
        "symfony/monolog-bridge": "^3.1",
        "symfony/console": "^3.1",
        "knplabs/console-service-provider": "^2.0",
        "pimple/pimple": "dev-feature/env-vars as 3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.6.*"
    },
    "autoload": {
        "psr-4": {
            "Toflar\\ComposerResolver\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Toflar\\ComposerResolver\\Tests\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Toflar/composer"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Toflar/Pimple"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
