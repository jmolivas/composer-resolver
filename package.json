{
  "name": "composer-resolver",
  "version": "1.0.0",
  "description": "Composer Resolver Desktop App",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "pack": "build -m --dir",
    "dist": "build -m",
    "postinstall": "install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Toflar/composer-resolver.git"
  },
  "author": "Yanick Witschi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Toflar/composer-resolver/issues"
  },
  "homepage": "https://github.com/Toflar/composer-resolver#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "browserify": "^13.0.1",
    "electron-builder": "^5.34.1",
    "electron": "^1.2.5",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^2.0.10",
    "gulp-concat": "^2.6.0",
    "gulp-postcss": "^6.1.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.4",
    "gulp-util": "^3.0.7",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "build": {
    "appId": "toflar.composer-resolver",
    "category": "public.app-category.developer-tools",
    "win": {
      "iconUrl": "https://github.com/toflar/composer-resolver/blob/master/build/icon.ico?raw=true"
    },
    "files": [
      "*/**",
      "!js${/*}",
      "!css${/*}"
    ],
    "protocols": [
      {
        "name": "Composer Resolver Desktop App",
        "schemes": [
          "composer-resolver"
        ]
      }
    ]
  }
}
